local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Gloudy Hub", HidePremium = false, SaveConfig = true, Introtext = "Gloudy Hub"})

local Tab = Window:MakeTab({
	Name = "Local",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "WalkSpeed"
})

Tab:AddTextbox({
	Name = "WalkSpeed",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end	  
})


local Section = Tab:AddSection({
	Name = "JumpLength"
})

Tab:AddTextbox({
	Name = "JumpLength",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end	  
})


Tab:AddButton({
	Name = "Ä°nfinite Yield",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/DarkNetworks/Infinite-Yield/main/latest.lua'))()
  	end    
})

Tab:AddButton({
	Name = "DarkDex",
	Callback = function()
      	loadstring(game:HttpGet('https://raw.githubusercontent.com/AlterX404/DarkDEX-V5/refs/heads/main/DarkDEX-V5'))()
  	end    
})

